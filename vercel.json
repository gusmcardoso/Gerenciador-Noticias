{
    "version": 2,
    "framework": null,
    "builds": [
        {
            "src": "public/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://seu-dominio-na-vercel.vercel.app",
        "APP_KEY": "@app-key",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "@db-host",
        "DB_PORT": "@db-port",
        "DB_DATABASE": "@db-database",
        "DB_USERNAME": "@db-username",
        "DB_PASSWORD": "@db-password"
    }
}